#!/usr/bin/env bash
# A Bash script that stops the 4-to_infinity_and_beyond process without using kill or killall

# Find the PID(s) of the 4-to_infinity_and_beyond process
pid=$(pgrep -f "4-to_infinity_and_beyond")

# Stop the process using the /proc interface
if [ -n "$pid" ]; then
    echo "$pid" | xargs -n 1 sudo /bin/kill -15
fi
